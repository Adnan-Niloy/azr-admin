@using Microsoft.AspNet.Identity
@using AzR.Admin.Web.ViewPage
@if (Request.IsAuthenticated)
{
    using (Html.BeginForm("LogOff", "UserAuth", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    {
    @Html.AntiForgeryToken()

    <ul class="nav navbar-nav navbar-right">
        <li>
           @* @Html.ActionLink("Hello " + CmsUser.Name + "!", "Index", "UserProfile", routeValues: null, htmlAttributes: new { title = "Manage" })*@
        </li>
        <li><a href="javascript:document.getElementById('logoutForm').submit()">Log out</a></li>
    </ul>
    }
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Register", "Register", "UserAuth", routeValues: null, htmlAttributes: new { id = "registerLink" })</li>
        <li>@Html.ActionLink("Log in", "Login", "UserAuth", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
    </ul>
}
